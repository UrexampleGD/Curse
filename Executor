gradient.Color = ColorSequence.new{ColorSequenceKeypoint.new(0, Color3.fromRGB(0,0,0)),ColorSequenceKeypoint.new(1, Color3.fromRGB(255,255,255))}
gradient.Rotation = 0
gradient.Parent = mainFrame
task.spawn(function()
 while true do
  for i = 0, 360, 1 do
   gradient.Rotation = i
   task.wait(0.04)
  end
 end
end)

